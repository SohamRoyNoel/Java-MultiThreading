package com.concurrency.main;

class Runner1 implements Runnable{

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.err.println("Runner1 : " + i);
		}		
	}
	
}

class Runner2 implements Runnable{

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("Runner2 : " + i);
		}		
	}
	
}
class Runner3 implements Runnable{

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("Runner3 : " + i);
		}		
	}
	
}
class Runner4 implements Runnable{

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("Runner4 : " + i);
		}		
	}
	
}
class Runner5 implements Runnable{

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("Runner5 : " + i);
		}		
	}
	
}
class Runner6 implements Runnable{

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("Runner6 : " + i);
		}		
	}
	
}
class Runner7 implements Runnable{

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("Runner7 : " + i);
		}		
	}
	
}

public class App {

	public static void main(String[] args) {
		
		Thread t1 = new Thread(new Runner1());
		Thread t2 = new Thread(new Runner2());
		Thread t3 = new Thread(new Runner3());
		Thread t4 = new Thread(new Runner4());
		Thread t5 = new Thread(new Runner5());
		Thread t6 = new Thread(new Runner6());
		Thread t7 = new Thread(new Runner7());
		
		t1.start();
		t2.start();
		t3.start();
		t4.start();
		t5.start();
		t6.start();
		t7.start();

	}

}
